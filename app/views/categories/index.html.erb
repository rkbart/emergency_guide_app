<%=flash[:notice]%>

<table>
  <thead>
    <th>Category</th>
    <th>Title</th>
    <th>Description</th>
  </thead>
  <tbody>

  <%@category.each do |category|%>

    <tr>
      <td><%=category.category %></td>
      <td><%=category.title%></td>
      <td><%=category.description%></td>
    </tr>

  <% end %>
  </tbody>
</table>

<div>

  <h3>Import User Data</h3>
  <%= form_tag import_categories_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>

<div>